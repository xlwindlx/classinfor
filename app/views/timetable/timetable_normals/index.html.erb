<p id="notice"><%= notice %></p>

<h1>Timetable Normals</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetable_normals.each do |timetable_normal| %>
      <tr>
        <td><%= link_to 'Show', timetable_normal %></td>
        <td><%= link_to 'Edit', edit_timetable_normal_path(timetable_normal) %></td>
        <td><%= link_to 'Destroy', timetable_normal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable Normal', new_timetable_normal_path %>
