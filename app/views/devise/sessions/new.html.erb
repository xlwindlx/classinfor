<%= form_for resource, as: resource_name, url: session_path(resource_name) do |f| %>

  <%= f.text_field :email, required: false, autofocus: true, placeholder: '아이디', class: 'input_id' %>
  <%= f.password_field :password, required: false, placeholder: '비밀번호', class: 'input_passwd'%>
  <%= f.submit '로그인', class: 'input_button' %>

<% end %>

<%= render "devise/shared/links" %>
